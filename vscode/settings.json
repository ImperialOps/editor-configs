{
  "telemetry.telemetryLevel": "off",
  "breadcrumbs.enabled": false,
  "extensions.ignoreRecommendations": true,
  "workbench.colorCustomizations": {
    // "editorError.foreground": "#3a2525",
    // "editorWarning.foreground": "#00000000",
    // "editorInfo.foreground": "#00000000",
  },
  "workbench.enableExperiments": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Default Dark+",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.autoReveal": false,
  "window.closeWhenEmpty": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "diffEditor.renderSideBySide": false,
  "diffEditor.renderIndicators": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.fontSize": 14,
  "editor.matchBrackets": "never",
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.glyphMargin": false,
  // This is for making the base-atelier-dune theme looks smooth on the variables
  "editor.tokenColorCustomizations": {
    //"variables": "#a6a28c",
    "comments": "#2ea542",
  },
  // Suggestions
  "editor.wordBasedSuggestions": "off",
  "editor.acceptSuggestionOnEnter": "on",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.roundedSelection": false,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.cursorStyle": "block",
  "editor.lineNumbers": "relative",
  "editor.cursorSurroundingLines": 10,
  "editor.cursorBlinking": "solid",
  "editor.folding": true,
  "editor.autoClosingBrackets": "never",
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 4,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "on",
  "editor.guides.indentation": false,
  // Vim setup
  "vim.enableNeovim": false,
  "vim.neovimUseConfigFile": true,
  "vim.searchHighlightColor": "rgba(150, 255, 255, 0.3)",
  "vim.leader": "<space>",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.cursorStylePerMode.visual": "block-outline",
  "vim.cursorStylePerMode.visualblock": "block-outline",
  "vim.cursorStylePerMode.visualline": "block-outline",
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": false,
  "vim.incsearch": true,
  "vim.handleKeys": {
    "<C-a>": true,
    "<C-d>": true,
    "<C-u>": true,
    "<C-f>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-u>"
      ],
      "after": [
        "<C-u>",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "<C-d>"
      ],
      "after": [
        "<C-d>",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "<leader>",
        "s"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "a"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.saveFiles"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "n"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.focusSideBar"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.toggleSidebarVisibility"
